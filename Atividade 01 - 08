*******************************************************************************/
#include <stdio.h>

int main()
{
    int num,res=0, c;
    
    printf("Digite um numero : \n");
    scanf("%d", &num);
    
    for(c=0;c<=10; c++){
        printf("%d ",res );
        res +=  num;
    }
    
    return 0;
}
